{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel",
    "add-module-exports",
    ["babel-plugin-styled-components",
      {
        "ssr": false
      }
    ],
    "@loadable/babel-plugin"
  ]
}